{
  "dashboard": {
    "title": "ONE-DATA Model (vLLM) 监控",
    "tags": ["model", "vllm", "mlops", "inference"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "服务状态",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"vllm-serving\"}",
            "legendFormat": "服务状态"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ],
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "当前加载的模型",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "count(vllm_model_loaded == 1)",
            "legendFormat": "已加载模型"
          }
        ]
      },
      {
        "id": 3,
        "title": "请求处理速率",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(vllm_requests_total[5m]))",
            "legendFormat": "请求/秒"
          }
        ]
      },
      {
        "id": 4,
        "title": "Token 生成速率",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(vllm_tokens_generated_total[5m]))",
            "legendFormat": "Tokens/秒"
          }
        ]
      },
      {
        "id": 5,
        "title": "请求速率",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(vllm_requests_total[5m]))",
            "legendFormat": "总请求"
          },
          {
            "expr": "sum(rate(vllm_requests_success_total[5m]))",
            "legendFormat": "成功"
          },
          {
            "expr": "sum(rate(vllm_requests_failed_total[5m]))",
            "legendFormat": "失败"
          }
        ],
        "yAxes": [
          {"label": "请求/秒"}
        ]
      },
      {
        "id": 6,
        "title": "Token 生成速率",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(vllm_tokens_generated_total[5m]))",
            "legendFormat": "预填充 Tokens/秒"
          },
          {
            "expr": "sum(rate(vllm_tokens_decoded_total[5m]))",
            "legendFormat": "解码 Tokens/秒"
          }
        ],
        "yAxes": [
          {"label": "Tokens/秒"}
        ]
      },
      {
        "id": 7,
        "title": "请求延迟分布",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(vllm_request_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "{{model}} P99"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(vllm_request_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "{{model}} P95"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(vllm_request_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "{{model}} P50"
          }
        ],
        "yAxes": [
          {"label": "延迟 (秒)"}
        ]
      },
      {
        "id": 8,
        "title": "首 Token 延迟 (TTFT)",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(vllm_time_to_first_token_bucket[5m])) by (le))",
            "legendFormat": "TTFT P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "max": 5,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 2, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Token 间延迟",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(vllm_time_per_output_token_bucket[5m])) by (le))",
            "legendFormat": "TPOT P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "max": 0.5,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.05, "color": "yellow"},
                {"value": 0.2, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "GPU 内存使用",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "vllm_gpu_memory_used_bytes / vllm_gpu_memory_total_bytes",
            "legendFormat": "GPU {{gpu}} 使用率"
          }
        ],
        "yAxes": [
          {"label": "使用率", "format": "percentunit", "min": 0, "max": 1}
        ]
      },
      {
        "id": 11,
        "title": "KV Cache 使用率",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "vllm_kv_cache_usage_bytes / vllm_kv_cache_total_bytes",
            "legendFormat": "KV Cache 使用率"
          }
        ],
        "yAxes": [
          {"label": "使用率", "format": "percentunit", "min": 0, "max": 1}
        ]
      },
      {
        "id": 12,
        "title": "GPU 利用率",
        "type": "heatmap",
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "vllm_gpu_utilization",
            "legendFormat": "GPU {{gpu}}"
          }
        ]
      },
      {
        "id": 13,
        "title": "请求排队",
        "type": "graph",
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "vllm_waiting_requests",
            "legendFormat": "等待请求数"
          },
          {
            "expr": "vllm_running_requests",
            "legendFormat": "运行请求数"
          }
        ],
        "yAxes": [
          {"label": "请求数"}
        ]
      },
      {
        "id": 14,
        "title": "各模型请求分布",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 38},
        "targets": [
          {
            "expr": "sum by (model) (vllm_requests_total)",
            "legendFormat": "{{model}}"
          }
        ]
      },
      {
        "id": 15,
        "title": "预填充 vs 解码时间",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 38},
        "targets": [
          {
            "expr": "sum(vllm_prefill_time_total)",
            "legendFormat": "预填充"
          },
          {
            "expr": "sum(vllm_decode_time_total)",
            "legendFormat": "解码"
          }
        ]
      },
      {
        "id": 16,
        "title": "错误类型分布",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 38},
        "targets": [
          {
            "expr": "sum by (error_type) (vllm_request_errors_total)",
            "legendFormat": "{{error_type}}"
          }
        ]
      },
      {
        "id": 17,
        "title": "请求生成 Token 数分布",
        "type": "heatmap",
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 46},
        "targets": [
          {
            "expr": "sum(rate(vllm_request_duration_seconds_sum[5m])) by (model, le) / sum(rate(vllm_request_duration_seconds_count[5m])) by (model, le)",
            "legendFormat": "{{model}}"
          }
        ]
      },
      {
        "id": 18,
        "title": "每秒 Token 数 (TPS) 按模型",
        "type": "graph",
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 46},
        "targets": [
          {
            "expr": "sum(rate(vllm_tokens_generated_total[5m])) by (model)",
            "legendFormat": "{{model}}"
          }
        ],
        "yAxes": [
          {"label": "Tokens/秒"}
        ]
      },
      {
        "id": 19,
        "title": "批处理大小",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 56},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(vllm_batch_size_bucket[5m])) by (le))",
            "legendFormat": "P95 批处理大小"
          },
          {
            "expr": "avg(vllm_batch_size)",
            "legendFormat": "平均批处理大小"
          }
        ],
        "yAxes": [
          {"label": "批大小"}
        ]
      },
      {
        "id": 20,
        "title": "显存碎片率",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 56},
        "targets": [
          {
            "expr": "vllm_memory_fragmentation_ratio",
            "legendFormat": "碎片率"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.3, "color": "yellow"},
                {"value": 0.5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 21,
        "title": "计算引擎效率",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 56},
        "targets": [
          {
            "expr": "vllm_compute_efficiency",
            "legendFormat": "计算效率"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.8, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 22,
        "title": "调度器决策延迟",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 62},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(vllm_scheduler_decision_latency_bucket[5m])) by (le))",
            "legendFormat": "P95 调度延迟"
          }
        ],
        "yAxes": [
          {"label": "延迟 (秒)"}
        ]
      },
      {
        "id": 23,
        "title": "迭代延迟",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 62},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(vllm_iteration_latency_bucket[5m])) by (le))",
            "legendFormat": "P95 迭代延迟"
          }
        ],
        "yAxes": [
          {"label": "延迟 (秒)"}
        ]
      },
      {
        "id": 24,
        "title": "模型性能对比表",
        "type": "table",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 68},
        "targets": [
          {
            "expr": "sum(rate(vllm_requests_total[5m])) by (model)",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "sources": [
              {
                "expr": "sum(rate(vllm_tokens_generated_total[5m])) by (model)",
                "name": "tokens_per_second"
              },
              {
                "expr": "histogram_quantile(0.95, sum(rate(vllm_request_duration_seconds_bucket[5m])) by (le, model))",
                "name": "p95_latency"
              }
            ]
          }
        ]
      }
    ]
  }
}
