{
  "realm": "one-data",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "roles": {
    "realm": [
      {
        "name": "admin",
        "description": "Administrator with full access to all resources"
      },
      {
        "name": "user",
        "description": "Regular user with standard access"
      },
      {
        "name": "viewer",
        "description": "Read-only access to most resources"
      },
      {
        "name": "data_engineer",
        "description": "Data engineer with access to data management features"
      },
      {
        "name": "data_analyst",
        "description": "Data analyst with read and export access"
      }
    ]
  },
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "email": "admin@one-data.com",
      "firstName": "Admin",
      "lastName": "User",
      "realmRoles": ["admin"],
      "clientRoles": {
        "bisheng-web": ["user"],
        "alldata-api": ["user"],
        "bisheng-api": ["user"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ]
    },
    {
      "username": "engineer",
      "enabled": true,
      "emailVerified": true,
      "email": "engineer@one-data.com",
      "firstName": "Data",
      "lastName": "Engineer",
      "realmRoles": ["data_engineer"],
      "clientRoles": {
        "bisheng-web": ["user"],
        "alldata-api": ["user"],
        "bisheng-api": ["user"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "engineer123",
          "temporary": false
        }
      ]
    },
    {
      "username": "analyst",
      "enabled": true,
      "emailVerified": true,
      "email": "analyst@one-data.com",
      "firstName": "Data",
      "lastName": "Analyst",
      "realmRoles": ["data_analyst"],
      "clientRoles": {
        "bisheng-web": ["user"],
        "alldata-api": ["user"],
        "bisheng-api": ["user"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "analyst123",
          "temporary": false
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "bisheng-web",
      "name": "Bisheng Web Frontend",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "bisheng-web-secret",
      "redirectUris": ["http://localhost:5173/*", "http://localhost:3000/*"],
      "webOrigins": ["http://localhost:5173", "http://localhost:3000"],
      "publicClient": false,
      "protocol": "openid-connect",
      "attributes": {
        "access.token.lifespan": "300"
      },
      "fullScopeAllowed": true,
      "protocolMappers": [
        {
          "name": "preferred_username",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "preferred_username",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "preferred_username",
            "jsonType.label": "String"
          }
        }
      ],
      "defaultRoles": ["user"]
    },
    {
      "clientId": "bisheng-api",
      "name": "Bisheng API",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "bisheng-api-secret",
      "serviceAccountsEnabled": true,
      "bearerOnly": false,
      "consentRequired": false,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect",
      "attributes": {
        "access.token.lifespan": "3600"
      },
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true
    },
    {
      "clientId": "alldata-api",
      "name": "Alldata API",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "alldata-api-secret",
      "serviceAccountsEnabled": true,
      "bearerOnly": false,
      "consentRequired": false,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect",
      "attributes": {
        "access.token.lifespan": "3600"
      },
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true
    },
    {
      "clientId": "realm-management",
      "name": "Realm Management",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "realm-management-secret",
      "bearerOnly": false,
      "consentRequired": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect",
      "attributes": {},
      "fullScopeAllowed": false,
      "protocolMappers": [
        {
          "name": "client-roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "user.attribute": "client-roles",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "client_roles",
            "jsonType.label": "String"
          }
        }
      ]
    }
  ],
  "browserSecurityHeaders": {
    "contentSecurityPolicyReportOnly": "",
    "xContentTypeOptions": "nosniff",
    "xRobotsTag": "none",
    "xFrameOptions": "SAMEORIGIN",
    "contentSecurityPolicy": "frame-src 'self'; frame-ancestors 'self'; object-src 'none';",
    "xXSSProtection": "1; mode=block",
    "strictTransportSecurity": "max-age=31536000; includeSubDomains"
  }
}
