{
  "version": "1.0",
  "name": "循环处理示例",
  "description": "对多个项目进行循环处理",
  "nodes": [
    {
      "id": "input-1",
      "type": "input",
      "position": { "x": 100, "y": 50 },
      "data": {
        "label": "输入",
        "config": { "key": "items" }
      }
    },
    {
      "id": "loop-1",
      "type": "loop",
      "position": { "x": 100, "y": 150 },
      "data": {
        "label": "循环处理",
        "config": {
          "loop_over": "{{ inputs.items }}",
          "max_iterations": 10,
          "output_mode": "all"
        }
      }
    },
    {
      "id": "tool-1",
      "type": "tool_call",
      "position": { "x": 100, "y": 280 },
      "data": {
        "label": "数据处理",
        "config": {
          "tool_name": "data_analysis",
          "input_from": "loop-1"
        }
      }
    },
    {
      "id": "output-1",
      "type": "output",
      "position": { "x": 100, "y": 400 },
      "data": {
        "label": "输出结果",
        "config": { "output_key": "results", "input_from": "loop-1" }
      }
    }
  ],
  "edges": [
    { "source": "input-1", "target": "loop-1" },
    { "source": "loop-1", "target": "tool-1" },
    { "source": "tool-1", "target": "output-1" }
  ]
}
