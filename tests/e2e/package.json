{
  "name": "one-data-studio-e2e",
  "version": "1.0.0",
  "description": "E2E tests for ONE DATA STUDIO - Deep Acceptance Testing",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:chromium": "playwright test --project=chromium-acceptance",
    "test:firefox": "playwright test --project=firefox-acceptance",
    "test:webkit": "playwright test --project=webkit-acceptance",
    "test:mobile": "playwright test --project=mobile-chrome",
    "test:report": "playwright show-report",
    "test:core": "playwright test core-pages-deep.spec.ts",
    "test:cube": "playwright test cube-deep.spec.ts",
    "test:data": "playwright test data-deep.spec.ts",
    "test:agent": "playwright test agent-deep.spec.ts",
    "test:admin": "playwright test admin-deep.spec.ts",
    "test:performance": "playwright test performance.spec.ts",
    "test:error": "playwright test error-handling-deep.spec.ts",
    "test:acceptance": "playwright test **/*-deep.spec.ts",
    "test:deep": "playwright test **/*-deep.spec.ts --headed",
    "test:fast": "playwright test --project=chromium-fast",
    "test:all": "playwright test --reporter=list",
    "acceptance": "./run-acceptance.sh",
    "acceptance:ui": "./run-acceptance.sh --ui",
    "acceptance:headed": "./run-acceptance.sh",
    "acceptance:headless": "./run-acceptance.sh --headless",
    "install:browsers": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
